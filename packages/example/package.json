{
  "name": "@perfyjs/examples",
  "version": "0.0.0",
  "description": "Examples of project using perfy",
  "scripts": {
    "perf:report": "perfy report",
    "perf:serve": "perfy serve",
    "perf:test": "protractor test/perf/protractor.conf.js",
    "perfy": "perfy --",
    "postinstall": "npm run webdriver-update",
    "lint:src": "tslint \"src/**/*.ts\" --type-check -p tsconfig.json",
    "lint:test": "tslint \"test/unit/**/*.ts\" --type-check -p test/tsconfig.json",
    "lint": "run-p lint:*",
    "serve": "node server.js",
    "start": "npm run serve",
    "test": "echo \"Skip: untested examples\" && exit 0",
    "webdriver-update": "webdriver-manager update"
  },
  "config": {
    "perfy": {
      "config": "test/perf/perfy.conf.js"
    }
  },
  "devDependencies": {
    "@angular/benchpress": "^0.1.0",
    "@perfyjs/cli": "^0.0.0",
    "@types/jasmine": "2.5.52",
    "@types/node": "^7.0.31",
    "awesome-typescript-loader": "^3.1.3",
    "html-webpack-plugin": "^2.28.0",
    "jasmine-core": "^2.6.4",
    "jasmine-spec-reporter": "^4.1.0",
    "npm-run-all": "^4.0.2",
    "portscanner": "^2.1.1",
    "protractor": "^5.1.2",
    "ts-node": "^3.0.6",
    "tslint": "^5.4.3",
    "typescript": "^2.3.4",
    "webpack": "^2.6.1",
    "webpack-dev-server": "^2.4.5"
  }
}
